/**index.wxss**/
.container{
  width: 90%;
  height: 600rpx;/*rpx是小程序的单位，与像素无关，可以自适应*/
  /* background-color:#cfcfcf; */
 background-color:fff;
  /* margin-left: 40rpx; */
  margin: 0 auto;/*居中*/
  margin-top: 20rpx;
  /* justify-content: center; */
  
  flex-wrap: wrap; 
   display: flex; 
  flex-direction: row; 
}
.index_item{
  width: 45%;
  height: 250rpx;
  border: 1rpx outset #fcfcfc;
  margin-left: 20rpx;
  margin-top: 30rpx;
  
  background-color:#fff;
  border-radius: 20%;
  text-align: center;
  color: #696969;
  
 
}
.index_item image{
  width: 100%;
  height: 200rpx;
  margin-top: 0rpx;
   border-radius: 10%;
}
.circle{
  width: 500rpx;
  height: 300rpx;
  
  border-radius: 20%;
}
.textStyle{
  width: 100%;
  margin-left: 50rpx;
   margin-top: 20rpx;
  
}
.buyHis{
  width: 100%;
  color: blue;
  text-align: center;
  margin-top: 50rpx;
  font-size:35rpx;
}
.buyHis image{
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;/*居中*/
  border-radius: 50%;
}